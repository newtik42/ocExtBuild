
<div class="col-sm-8 col-sm-offset-2">
<div class="about-info eula">
    <div class="logo"><center><a href="{{ text_COPYRIGTH }}" target="_blank"><img src="data:image/png;base64,"
                                                                                  style="height: 200px" alt="{{ text_COPYRIGTH }}"></a></center></div>
    <div class="form-group">
        <label class="col-sm-4 control-label" >{{ text_version }}</label>
        <div class="col-sm-8 inline-text">
            {{ version }} 
        </div>	
    </div>
    {% if haslicense %}
        <div class="form-group">
            <label class="col-sm-4 control-label" for="input-licence-key">{{ text_label_licence_key }}</label>
            <div class="col-sm-8">
                <input type="text" id="key" name="licence_key" value="" placeholder="{{ text_label_licence_key }}" id="licence-key" 
                       class="form-control"  />
            </div>
        </div>	
    {% endif %}
    <div class="form-group">
        <label class="col-sm-4 control-label" for="input-theme_technics_label_sale_d">{{ text_support }}</label>
        <div class="col-sm-8 inline-text">
            <a href="mailto:{{ support.email }}" target="_blank">Email:{{ support.email }}</a><br>
            <a href="https://t.me/{{ support.telegram }}" target="_blank">Telegram: @{{ support.telegram }}</a><br>
        </div>	
    </div>	
    <div class="form-group">
        <label class="col-sm-4 control-label" for="input-theme_technics_label_sale_d">{{ text_developer }}</label>
        <div class="col-sm-8 inline-text">
            {{ text_COPYRIGTHSHOWLINK }}
        </div>	
    </div>	
    <div class="terms"><h1><center>{{ text_licence_1 }}{{ module_name }}</center></h1>{{ text_licence_2 }}{{ text_COPYRIGTH }}{{ text_licence_3 }}</div>
    <div class="form-group">
        <div class="col-12 inline-text text-right">
        <label>{{ text_checkbox_label }}<input eula type="checkbox"  value="1"></label> &nbsp;
        <button type="button" disabled onclick='$("[name=\"apply\"]").attr("value", 1);$("#form-module").submit();' class="btn btn-primary" >
            {{ text_checkbox_butt }}
        </button>
        </div>
    </div>
</div>	
        <script>
            $(document).ready(function(){
                $('[eula][type="checkbox"]').click(function() {
                    var button = $('[type="button"]');
                    if ($(this).is(":checked")) {
                        button.removeAttr('disabled');
                    } else {
                        button.attr('disabled', 7);
                    }
                });
            });
        </script>

